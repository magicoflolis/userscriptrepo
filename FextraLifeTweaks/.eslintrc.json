{
  "root": true,
  "globals": {
    "GM": "writable"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "greasemonkey": true,
    "jquery": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "eslintIgnore": ["header.js","userscript.js"],
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaFeatures": {
      "globalReturn": true,
      "arrowFunctions": true,
      "modules": true
    },
    "ecmaVersion": "latest"
  },
  "rules": {}
}